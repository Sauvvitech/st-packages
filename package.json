{
  "description": "A package from manager packages for sauvvitech services",
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "clean": "rm -rf lib",
    "build": "yarn clean && tsc",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:int": "jest --runInBand --forceExit --config ./jest/jest.int-config.js",
    "test:unit": "jest --config ./jest/jest.config.js",
    "reinstall": "rm -rf node_modules && yarn",
    "prepublish": "yarn test && yarn build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepare": "yarn lint ; yarn build",
    "release": "yarn run semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Sauvvitech/st-packages.git"
  },
  "keywords": [],
  "author": "Lucas Silva",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.14",
    "@types/kafkajs": "^1.9.0",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "prettier": "^3.5.0",
    "semantic-release": "^24.2.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.6",
    "amqplib": "^0.10.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-openapi-validator": "^4.13.8",
    "helmet": "^8.0.0",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.8.3",
    "traceability": "^3.2.2"
  }
}
